    "vue-i18n": "^8.26.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.6.1",
    "vuex": "^3.4.0",
    "vuex-persist": "^3.1.3"
